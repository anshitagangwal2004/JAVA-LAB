import java.util.*;
public class SumOfArrElement
{
public static void main(String[] args) {
   Scanner sc=new Scanner(System.in);
   System.out.print("Enter the range of an array:    ");
   int n=sc.nextInt();
   int s[]=new int[n];
   int t=0;
   System.out.println("Enter elements: ");
   for(int i=0;i<n;i++){
       s[i]=sc.nextInt();
   }
   for(int i=0;i<n;i++){
       t+=s[i];
   }
   System.out.println("Sum of elements=    "+t);
   }
}
